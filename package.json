{
  "name": "vue-handy-ga",
  "version": "1.1.0",
  "description": "Vue.js plugin making Google Analytics integration handy!",
  "main": "dist/vue-handy-ga.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "exit 0;",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "example:basic": "poi -s examples/basic/index.js",
    "example:builtin": "poi -s examples/builtin-consent/index.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint": "npx eslint 'src/**/*.{js,vue}'",
    "lintfix": "lint -- --fix",
    "lint:examples": "npx eslint 'examples/**/*.{js,vue}'",
    "lintfix:examples": "lint:js -- --fix"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "npx eslint . --fix",
      "git add"
    ],
    "examples/**/*.{js,vue}": [
      "npx eslint . --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+/pedraal/vue-handyga.git"
  },
  "keywords": [
    "vue",
    "analytics",
    "google"
  ],
  "author": "Pierre Golfier <pgolfier.pro@gmail.com> (https://pedraal.dev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pedraal/vue-handyga/issues"
  },
  "homepage": "https://github.com/pedraal/vue-handyga#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "node-sass": "^4.12.0",
    "poi": "^12.7.1",
    "sass-loader": "^8.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue": "^2.6.10",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.0.4",
    "vuex": "^3.1.1",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "js-cookie": "^2.2.1"
  }
}
